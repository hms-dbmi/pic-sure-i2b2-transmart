
## labeling
STACK_ENV=dev
STACK_NAME=pic-sure-i2b2tm

GUEST_AUTO_LOGIN=true
GUEST_USER=admin

# i2b2transmart database
DB_HOST=
DB_USER=BIOMART_USER
DB_PASSWORD=biomart_user
DB_PORT=1521
DB_DB=ORCL

# tabs
HIDE_SEARCH=true
HIDE_BROWSE=true
HIDE_SAMPLE=true
HIDE_GENE_SIG=true
HIDE_GWAS=true
HIDE_UPLOAD=true
HIDE_EXPORT=true
HIDE_EXPORT_JOBS=true
HIDE_ANALYSIS=true
HIDE_WORKSPACE=true

# fractalis
FRACTALIS_ACTIVE=true
FRACTALIS_NODE=/fractalis
FRACTALIS_DATA_SOURCE=http://httpd/rest/v1
FRACTALIS_RESOURCE_NAME=i2b2-wildfly-default/Demo

# i2b2-wildfly
I2B2HIVE=demouser
I2B2DEMODATA=demouser
I2B2METADATA=demouser
I2B2PM=demouser
I2B2WORKDATA=demouser

# required for JWT tokens between i2b2/tranSMART <-> PIC-SURE
CLIENT_SECRET=dummysecret
CLIENT_ID=dummyid

# pic-sure
IRCT_DB_HOST=irctdb
IRCT_DB_PORT=3306
IRCT_DB_CONNECTION_USER=root
IRCT_DB_PASSWORD=password
# use aud (audience) claim in JWT token
IRCT_USER_FIELD=sub

# HPDS settings
HEAPSIZE=1024
CACHESIZE=500
SMALL_TASK_THREADS=1
LARGE_TASK_THREADS=1
SMALL_JOB_LIMIT=100
ID_BATCH_SIZE=50000
SURVIVOR_RATIO=255

# i2b2transmart emails
# notification
NOTIFICATION_EMAILS=avillach_lab_developers@googlegroups.com

# support
CONTACT_US=avillach_lab_developers@googlegroups.com
BUG_REPORT_URL=https://jira.transmartfoundation.org/projects/I2B2TM
USER_GUIDE_URL=http://s3.amazon.com/dbmi-public-docs/i2b2_transmart_userguide.pdf

AUTH_CLIENT_SECRET=secret
AUTH_CLIENT_ID=client_id
AUTH_USER_ID_CLAIM=sub
AUTH0TOKEN=nothing
AUTH0HOST=https://avillachlab.auth0.com

# Specify a container or qualified DB address
AUTH_MYSQLADDRESS=picsureauthdb
AUTH_DB_PORT=3306
AUTH_DB_DATABASE=auth
AUTH_DB_CONNECTION_USER=root
AUTH_MYSQLPASS=password

# ************ picsure2.env ****************
# mysql database
# Specify a container or qualified DB address
PICSURE2_MYSQLADDRESS=
PICSURE2_DB_PORT=3306
PICSURE2_DB_CONNECTION_USER=root
PICSURE2_MYSQLPASS=password

# JWT Token
CLIENT_ID=
CLIENT_SECRET=
picsure_USER_FIELD=sub

# ***** hpds settings **********
HEAPSIZE=2048
CACHESIZE=2000
SMALL_TASK_THREADS=1
LARGE_TASK_THREADS=1
SMALL_JOB_LIMIT=100
ID_BATCH_SIZE=50000
SURVIVOR_RATIO=5
